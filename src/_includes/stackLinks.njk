<ul>
  {% for stackLink in global.stackLinks %}
    <li class="p4 flex align-middle justify-center">
      <div class="flex align-middle justify-center m-2 p-2 w-80 bg-cyan-700 shadow-md">
        <img class="fill-black stroke-black size-6" src="{{ stackLink.iconPath }}"></img>
        <p class="pl-2">{{ stackLink.text }}</p>
      </div>
    </li>
  {% endfor %}
</ul>